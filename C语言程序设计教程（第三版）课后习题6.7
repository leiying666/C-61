#include<stdio.h>

int main()
{
	int n,i,k;
	scanf("%d", &n);
	for (k = 2; k <= n; k++)
	{
		int c = 0, sum = 0, a[100] = {0};
		for (i = 1; i <= k / 2; i++)
		{
			if (k % i == 0)
			{
				sum += i;
				c++;
				a[c - 1] = i;
			}
		}
		if (sum == k)
		{
			printf("%d its factors are ", k);
			for (int j = 0; j < c; j++)
			{
				if (j != c - 1)
				{
					printf("%d ", a[j]);
				}
				else
				{
					printf("%d\n", a[j]);
				}
			}
		}
	}
	return 0;
}
